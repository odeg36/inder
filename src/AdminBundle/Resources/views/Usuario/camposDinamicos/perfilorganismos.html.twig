{% block perfil_organismos_type_row %}
    <div class="col-md-12">
        <div class="campoGrisClaro row">
            <div class="col-xs-12 col-md-12">
                <div class="col-xs-12 col-sm-6 col-md-6">
                    {{ form_label(form.documento) }}
                    {{ form_widget(form.documento, {'attr': {'class': 'form-control'} }) }}
                    {% include 'AdminBundle:CRUD:errores_campos.html.twig' with { errors : form.documento.vars.errors }%}
                </div>
                <div class="col-xs-12 col-sm-6 col-md-6">
                    {{ form_label(form.nombre) }}
                    {{ form_widget(form.nombre, {'attr': {'class': 'form-control'} }) }}
                    {% include 'AdminBundle:CRUD:errores_campos.html.twig' with { errors : form.nombre.vars.errors }%}
                </div>
            </div>
            <div class="col-xs-12 col-md-12">
                <div class="col-xs-12 col-sm-6 col-md-6">
                    {{ form_label(form.perfil) }}
                    {{ form_widget(form.perfil, {'attr': {'class': 'select-perfil form-control'} }) }}
                    {% include 'AdminBundle:CRUD:errores_campos.html.twig' with { errors : form.perfil.vars.errors }%}
                </div>
                <div class="col-xs-12 col-sm-6 col-md-6">
                    {{ form_label(form.telefono) }}
                    {{ form_widget(form.telefono, {'attr': {'class': 'form-control'} }) }}
                    {% include 'AdminBundle:CRUD:errores_campos.html.twig' with { errors : form.telefono.vars.errors }%}
                </div>
            </div>
            <div class="col-xs-12 col-md-12">
                <div class="col-xs-12 col-sm-6 col-md-6">
                    {{ form_label(form.correo) }}
                    {{ form_widget(form.correo, {'attr': {'class': 'form-control'} }) }}
                    {% include 'AdminBundle:CRUD:errores_campos.html.twig' with { errors : form.correo.vars.errors }%}
                </div>
            </div>
        </div>
    </div>
{% endblock %}
{% block perfil_organismos_type_widget %}
    {{ form_widget(form) }}
    <br/>
    <p class="text-center">
        <a href="#" class="collection-remove btn btn-default">-</a>
        <a href="#" class="collection-add btn btn-default">++</a>
    </p>
{% endblock %}