{% block componente_type_row %}
    <div class="campoGris row  margenPasos componente-hijo-eliminar margen-bottom-10 " id="form_componente_{{ form.vars.name }}" data-id="{{ form.vars.name }}">
        <div class="col-md-6">
            {{form_label(form.componente)}}
            {{form_widget(form.componente)}}
            {% include 'AdminBundle:CRUD:errores_campos.html.twig' with { errors : form.componente.vars.errors }%}
        </div>
        <div class="col-md-6">
            {{form_label(form.nivel)}}
            {{form_widget(form.nivel)}}
            {% include 'AdminBundle:CRUD:errores_campos.html.twig' with { errors : form.nivel.vars.errors }%}
        </div>
        {{ form_rest(form) }}        
        <div class="col-md-12 collection-remove collection-action btnCentrar "  >
            <div class="col-md-4 btnCentrar campoGris">
                <button type="button" class="btn btn_remover btnRojo">
                    <i class="fa fa-minus-circle" aria-hidden="true"></i>Quitar este componente
                </button>
            </div>
        </div>
    </div>

